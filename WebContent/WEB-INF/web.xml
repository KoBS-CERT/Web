<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EZ2C</display-name>
  <welcome-file-list>
    <welcome-file>/Resources/production/index.htmlproduction/index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>/Resources/Test/index2.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
    <error-page><!--403에러처리-->
    <error-code>403</error-code>
    <location>/page_403.html</location>
  </error-page>
    <error-page><!--404에러처리-->
    <error-code>404</error-code>
    <location>/page_404.html</location>
  </error-page>
  
  <error-page><!--500에러처리-->
    <error-code>500</error-code>
    <location>/page_500.html</location>
  </error-page>
</web-app>